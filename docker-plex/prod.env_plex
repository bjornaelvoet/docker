TZ=Europe/Brussels
# This is the ip lex is listening on
# In bridged mode, we need to map these ports to the docker host
ADVERTISE_IP=https://192.168.1.2:32400
# We dont't use this, we ssh into the plex host with port forwarding -L 32400:localhost:32400 and browse to localhost:32400 to claim
PLEX_CLAIM=
#Matching ids for plex config folder
PLEX_UID=1000
PLEX_GID=1001
CHANGE_CONFIG_DIR_OWNERSHIP=false
# We don't allow anymore for unsecure networks
ALLOWED_NETWORKS=
